{
  "name": "@bankr/cli",
  "version": "0.1.0-beta.10",
  "description": "Official CLI for the Bankr AI agent platform",
  "type": "module",
  "bin": {
    "bankr": "./dist/cli.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && chmod +x dist/cli.js",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "prebuild": "bun run clean",
    "prepublishOnly": "bun run build",
    "start": "bun run ./src/cli.ts"
  },
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "bankr",
    "cli",
    "ai",
    "defi",
    "web3",
    "trading",
    "blockchain"
  ],
  "author": "Bankr",
  "license": "MIT",
  "homepage": "https://bankr.bot",
  "bugs": {
    "url": "https://discord.gg/bankr"
  },
  "dependencies": {
    "@inquirer/prompts": "^8.2.0",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "open": "^10.1.0",
    "ora": "^8.1.0"
  }
}
